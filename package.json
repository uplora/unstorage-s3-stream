{
  "name": "@uplora/unstorage-s3-stream",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@10.27.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uplora/unstorage-s3-stream.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "release": "pnpm build && changelogen --release --push --hideAuthorEmail && pnpm publish --access public",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:deps": "madge --circular --extensions ts ./src",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "prepare": "husky"
  },
  "peerDependencies": {
    "unstorage": "^1.16.0 || ^2.0.0-alpha.4"
  },
  "dependencies": {
    "minio": "^8.0.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "madge": "^8.0.0",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1"
  },
  "lint-staged": {
    "*.ts": "pnpm run lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
